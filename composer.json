{
    "name": "kunstmaan/config-bundle",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Config bundle supplies your project with custom configuration entities that can be managed from within KunstmaanCMS",
    "keywords": ["kunstmaan", "cms", "admin", "config"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanConfigBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "kunstmaan/admin-bundle": "~5.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Kunstmaan\\ConfigBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-6.0": "6.0-dev"
        }
    }
}
